<idea-plugin>
  <id>com.your.company.unique.plugin.id</id>
  <name>Visual Studio Code Dark Plus Theme</name>
  <version>1.0</version>
  <vendor email="sam@rmcreative.ru" url="https://rmcreative.ru">Alexander Makarov</vendor>

  <description><![CDATA[
      Dark theme inspired by Visual Studio Code Dark Plus

      <img src="https://raw.githubusercontent.com/samdark/intellij-visual-studio-code-dark-plus/master/screenshot.png" border="0" width="700"/>
    ]]></description>

  <change-notes><![CDATA[
    <b>1.0</b>
    <ul>
        <li>Initial release</li>
    </ul>
    ]]>
  </change-notes>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
  <idea-version since-build="173.0"/>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
       on how to target different products -->
  <!-- uncomment to enable plugin in all products
  <depends>com.intellij.modules.lang</depends>
  -->

  <extensions defaultExtensionNs="com.intellij">
    <!-- Add your extensions here -->
    <themeProvider id="e843a6a0-d4df-4c2a-b5b6-99dcbb839b8d" path="/visual_studio_code_dark_plus.theme.json"/>
  </extensions>

  <actions>
    <!-- Add your actions here -->
  </actions>

</idea-plugin>